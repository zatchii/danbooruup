{
	"manifest_version": 2,
	"name": "DanbooruUp",
	"version": "0.4.0",
	"description": "Tag completion for the Danbooru web site.",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"homepage_url": "http://pianosite.net/danbooruup/",
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"permissions": [
		"http://danbooru.donmai.us/*",
		"unlimitedStorage",
		"contextMenus",
		"tabs"
	],
	"content_scripts": [{
		"matches": ["http://danbooru.donmai.us/*"],
		"js": ["inject.js"]
	}],
	"update_url": "http://pianosite.net/danbooruup/update_c.xml"
}
